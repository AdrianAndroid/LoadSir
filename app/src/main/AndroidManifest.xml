<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.joyy.loadsir">

    <application
        android:name="com.joyy.loadsir.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.joyy.loadsir.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name="com.joyy.loadsir.target.NormalActivity"/>
        <activity android:name="com.joyy.loadsir.target.ConvertorActivity"/>
        <activity android:name="com.joyy.loadsir.target.FragmentSingleActivity"/>
        <activity android:name="com.joyy.loadsir.target.ViewTargetActivity"/>
        <activity android:name="com.joyy.loadsir.target.DefaultCallbackActivity"/>
        <activity android:name="com.joyy.loadsir.target.MultiFragmentWithViewPagerActivity"/>
        <activity android:name="com.joyy.loadsir.target.MultiFragmentActivity"/>
        <activity android:name="com.joyy.loadsir.target.PlaceholderActivity"/>
        <activity android:name="com.joyy.loadsir.target.AnimateActivity"/>
        <activity android:name="com.joyy.loadsir.target.KeepTitleFragmentActivity"/>
        <activity android:name="com.joyy.loadsir.target.BestPracticesActivity"/>
        <activity android:name="com.joyy.loadsir.target.LottieActivity"/>
        <activity
            android:name="com.joyy.loadsir.target.KeepTitleActivity"
            android:theme="@style/NoTitle"/>
        <activity android:name="com.joyy.loadsir.target.ConstraintLayoutActivity"/>
    </application>

</manifest>